@model IList<ShareModel.CategoryModel>

    <ul class="nav-navbar-nav flex-grow-1">
        @foreach (var category in Model)
        {
        <li class="nav-item" style=" display: inline-block;">
            <a class="nav-link text-dark" asp-controller="RealEstate" asp-action="CategoryById" asp-route-categoryName="@category.CategoryName">@category.CategoryName</a>
        </li>
        }
    </ul>
